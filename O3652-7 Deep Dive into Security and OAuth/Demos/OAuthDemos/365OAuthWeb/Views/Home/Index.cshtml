<div style="margin:25px;">
    <table>
        <thead>
        <th>Last Name</th>
        <th>First Name</th>
        <th>Job Title</th>
        <th>Work E-mail</th>
        <th>Work Phone</th>
        @foreach (var Contact in ViewBag.Contacts)
        {
            <tr>
                <td>
                    <div style="width:200px;">@Contact.Surname</div>
                </td>
                <td>
                    <div style="width:200px;">@Contact.GivenName</div>
                </td>
                <td>
                    <div style="width:200px;">@Contact.JobTitle</div>
                </td>
                <td>
                    <div style="width:400px;">@Contact.EmailAddress1</div>
                </td>
                <td>
                    <div style="width:200px;">@Contact.BusinessPhone1</div>
                </td>
            </tr>
        }
    </table>
    </
</div>
