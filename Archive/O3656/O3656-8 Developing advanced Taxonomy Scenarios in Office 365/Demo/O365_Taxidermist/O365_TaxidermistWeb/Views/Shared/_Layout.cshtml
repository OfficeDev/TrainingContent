<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=10" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>@ViewBag.Title</title>
  @Styles.Render("~/Content/css")
  <link href="~/Content/themes/base/jquery.ui.all.css" rel="stylesheet" />
  <link href="~/Content/jquery.treeview.css" rel="stylesheet" />
  @Scripts.Render("~/bundles/modernizr")
</head>

<body>
  <div id="page_width" class="container">

    <div id="top_nav" class="row">
      <a href="@Html.Raw(Request["SPHostUrl"])">Back to Host Web</a>
    </div>

    <div id="page_header" class="row">
      <h2 id="page_title">Office 365 Taxidermists</h2>
    </div>

    <div id="main_nav" class="row">
      @Html.ActionLink("Home", "Index", "Home")
      @Html.ActionLink("Create Termset", "CreateTermset", "Home")
    </div>

    <div class="row body-content">
      @RenderBody()
    </div>

  </div>

  @Scripts.Render("~/bundles/jquery")
  @Scripts.Render("~/bundles/bootstrap")
  @Scripts.Render("~/bundles/spcontext")
  @RenderSection("scripts", required: false)
  <script src="~/Scripts/jquery-ui-1.9.0.js"></script>
  <script src="~/Scripts/jquery-treeview.js"></script>
  <script src="~/Scripts/app.js"></script>
</body>
</html>